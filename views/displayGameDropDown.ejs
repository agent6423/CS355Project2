<% include header %>


<% if(rs.length > 0) { %>
This example form submits without using ajax.  All of your forms should use Ajax.
<form method="get" action="/course">
    <label for="courseid">Courses</label>
    <select id="courseid" name="courseid">
        <% for (var i=0; rs.length > i; i++) { %>
        <option value="<%= rs[i].Course_number %>">
            <%= rs[i].Name %>
        </option>
        <% } %>
    </select>
    <input type="submit">
</form>
<% } else { %>
The table does not contain any records.
<% } %>

<% include footer %>
